class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for num in nums:
            if ((target - num) in nums) and (target - num != num):
                return [nums.index(num), nums.index (target-num)]
        return []
        
